<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <system.webServer>
        <rewrite>
			<rule name="rule 1e">
				<match url="^(.*)$"  />
				<action type="Rewrite" url="/e.php?{R:1}"  />
			</rule>
			<rule name="rule 2e">
				<match url="^(.*)/$"  />
				<action type="Rewrite" url="/e.php?{R:1}"  />
			</rule>
        </rewrite>
    </system.webServer>
</configuration>